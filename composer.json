{
    "name": "gdebrauwer/laravel-hateoas",
    "description": "This package provides an easy way to create HATEOAS links",
    "keywords": [
        "gdebrauwer",
        "laravel-hateoas"
    ],
    "homepage": "https://github.com/gdebrauwer/laravel-hateoas",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "GÃ¼nther Debrauwer",
            "email": "debrauwer.gunther@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "GDebrauwer\\LaravelHateoas\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GDebrauwer\\LaravelHateoas\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GDebrauwer\\LaravelHateoas\\LaravelHateoasServiceProvider"
            ],
            "aliases": {
                "LaravelHateoas": "GDebrauwer\\LaravelHateoas\\LaravelHateoasFacade"
            }
        }
    }
}
